{
  "name": "storybook-tutorial",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "update-stories": "sb-rn-get-stories --config-path .storybook",
    "storybook-watcher": "sb-rn-watcher --config-path .storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook-android": "yarn update-stories && STORYBOOK_ON=true yarn android",
    "storybook-ios": "yarn update-stories && STORYBOOK_ON=true yarn ios",
    "storybook-generate": "sb-rn-get-stories"
  },
  "dependencies": {
    "expo": "~52.0.25",
    "expo-constants": "~17.0.4",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.0.1",
    "@react-native-community/slider": "^4.5.2",
    "@storybook/addon-ondevice-actions": "^8.4.4",
    "@storybook/addon-ondevice-controls": "^8.4.4",
    "@storybook/react-native": "^8.4.4",
    "@types/react": "~18.3.12",
    "babel-loader": "^8.3.0",
    "react-dom": "18.3.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "^3.16.6",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
