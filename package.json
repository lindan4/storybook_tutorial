{
  "name": "storybook-tutorial",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook-android": "yarn update-stories && STORYBOOK_ON=true yarn android",
    "storybook-ios": "yarn update-stories && STORYBOOK_ON=true yarn ios"
  },
  "dependencies": {
    "expo": "~51.0.8",
    "expo-constants": "~16.0.1",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^8.0.1",
    "@react-native-community/slider": "^4.5.2",
    "@storybook/addon-ondevice-actions": "^7.6.18",
    "@storybook/addon-ondevice-controls": "^7.6.18",
    "@storybook/react-native": "^7.6.18",
    "@types/react": "~18.2.45",
    "babel-loader": "^8.3.0",
    "react-dom": "18.2.0",
    "react-native-safe-area-context": "^4.10.1",
    "typescript": "~5.3.3"
  },
  "private": true
}
